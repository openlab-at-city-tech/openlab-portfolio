{
  "name": "openlab-at-city-tech/openlab-portfolio",
  "description": "OpenLab ePortolio feature",
  "type": "wordpress-plugin",
  "license": "GPL-2.0-or-later",
  "require": {
    "php": "~5.6|~7.0"
  },
  "autoload": {
    "psr-4": {
      "OpenLab\\Portfolio\\": "src"
    },
    "files": [
      "src/polyfills.php",
      "src/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
